<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name , class: 'form-control' %><br>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %><br>

      <%= f.label :company %>
      <%= f.text_field :company , class: 'form-control' %><br>

       <%= f.label :country %>
      <%= f.text_field :country , class: 'form-control'%><br>

      <%= f.label :password %>
      <%= f.password_field :password , class: 'form-control'%><br>

      <%= f.label :passwordconf, "Confirmation" %>
      <%= f.password_field :passwordconf , class: 'form-control'%><br>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>


